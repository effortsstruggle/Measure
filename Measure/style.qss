/*主窗体点击按钮背景*/
QComboBox::drop-down {
    border:0;
    background-image:url(:/staticFile/images/icon_05.png);
    width:30px;
}
QComboBox QAbstractItemView {
    padding-left:12;
    outline: 1px solid rgba(98,137,192,0.4);/*选中项外边框*/
    selection-background-color:rgba(98,137,192,0.4);/* 选中项的背景色 */
    border: 1px solid rgba(98,137,192,0.1);/* 整个下拉窗体的边框 */
    background-color: #6289c0; /* 整个下拉窗体的背景色 */
    selection-color: #ffffff;/*下拉框选中项字体颜色*/
}


