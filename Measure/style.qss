/*------------实时监测页面样式---------------*/
/*取消QTabWidget的白色背景*/
#actual_tab::pane{
    border:none;
}
#actual_tab QStackedWidget{
    background: transparent;
}

#actual_tab QTabBar::tab
{
    /*background-color: #0B0E11;*/
    background: transparent;
    border :1px solid RGB(39,55,108);
    font-family:Consolas;    /*设置tab中的文本的字体*/
    font-size:13pt;
    color:#ced4da;    /*设置tab中的文本的颜色*/
    border-radius: 5px;    /*设置tab的边框的圆角（左上圆角）*/
    /*border-top-right-radius: 5px;    设置tab的边框的圆角（右上圆角）*/
    min-width: 100px;
    min-height: 25px;
    padding: 5px;
    margin-left: 20px;/*设置tab的间距*/
}
#actual_tab QTabBar::tab:selected
{
    background-color: rgb(108, 117, 125, 65);
}


